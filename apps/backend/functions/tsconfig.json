{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
		"lib": ["ES2016", "dom"],
		"baseUrl": ".",
		"paths": {
			"@utils/*": ["./_utils/*"],
			"@types/*": ["./_types/*"],
			"@gql": ["./_gql/index.ts"]
		}
  }
}
